#include <stdio.h>
#include <errno.h>
#include <string.h>

int main(int argc, char *argv){
    int v = atoi(argv[1]);
    int statut;
    statut = syscall(336);
    if (statut == -1){
        fprintf(stderr, "%s : sc336 : %s\n", argv[0], strerror(errno));
        exit(1)
    }
    statut = syscall(335,v);
    if (statut == -1){
        fprintf(stderr, "%s : sc335 : %s\n", argv[0], strerror(errno));
        exit(1);
    }
    else{
        printf("%d +1 = %d\n", v, statut);
        return 0;
    }

}